{
  "name": "@react-ol/fiber",
  "version": "1.0.0-beta.1",
  "sideEffects": false,
  "description": "React OpenLayers bindings using react fiber",
  "license": "MIT",
  "type": "module",
  "author": "Vincent Lecrubier <vincent.lecrubier@gmail.com>",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/ts/index.d.ts"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "type-check": "tsc",
    "test": "jest",
    "build": "swc ./src --out-dir ./dist/esm --config-file ./esm.swcrc && swc ./src --out-dir ./dist/cjs --config-file ./cjs.swcrc && tsc -d --declarationDir dist/ts --declarationMap --emitDeclarationOnly"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "react-reconciler": "0.25.1",
    "scheduler": "0.20.1"
  },
  "peerDependencies": {
    "ol": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@swc/cli": "0.1.56",
    "@swc/core": "1.2.159",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@types/jest": "24.0.13",
    "@types/lodash": "4.14.154",
    "@types/proj4": "2.5.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-reconciler": "0.26.1",
    "jest": "27.5.1",
    "next": "12.1.1",
    "ol": "6.13.0",
    "react": "latest",
    "react-dom": "latest",
    "tsc": "2.0.4",
    "typescript": "4.3.5"
  }
}
